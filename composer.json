{
  "name": "thepixeldeveloper/sitemap-bundle",
  "type": "library",
  "description": "A Symfony Bundle that integrates thepixeldeveloper/sitemap.",
  "keywords": ["sitemap", "xml", "symfony", "bundle"],
  "license": "MIT",
  "authors": [
    {
      "name": "Mathew Davies",
      "homepage": "http://mathew-davies.co.uk",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "gitlab",
      "url": "https://www.devkit.net/thepixeldeveloper/sitemap.git"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "mikey179/vfsStream": "^1.6",
    "phpunit/phpunit": "^6.4",
    "symfony/console": "^3.3",
    "symfony/framework-bundle": "^3.3",
    "symfony/yaml": "^3.3",
    "thepixeldeveloper/sitemap": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Thepixeldeveloper\\SitemapBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Thepixeldeveloper\\SitemapBundle\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "coverage": "phpunit --coverage-text --colors=never --coverage-html ./coverage"
  },
  "config": {
    "sort-packages": true,
    "gitlab-domains": [
      "www.devkit.net"
    ]
  }
}
